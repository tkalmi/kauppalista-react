{
  "name": "Kauppalista-app-react",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "6.2.2"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "npm run web",
    "build-prod": "npm run build-prod-server && npm run build-prod-client",
    "build-prod-client": "cross-env NODE_ENV=production webpack --config webpack.config.js -p --progress",
    "build-prod-server": "babel -d ./server_build ./server -s",
    "web": "npm run build-prod && cross-env NODE_ENV=production node server_build/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node --presets es2015,stage-2 server/server.js"
  },
  "keywords": [
    "kauppalista",
    "mongo",
    "nodejs",
    "react",
    "redux",
    "socket.io"
  ],
  "author": "Tomi Kalmi",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "body-parser": "1.15.2",
    "cross-env": "^3.1.3",
    "express": "4.14.0",
    "express-session": "^1.14.2",
    "immutable": "3.8.1",
    "mongoose": "^4.7.1",
    "morgan": "1.7.0",
    "react": "^15.4.0",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.2",
    "socket.io": "^1.7.1",
    "socket.io-client": "^1.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
